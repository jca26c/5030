def scalene(a,b,c):
    if a+b>c:
        print('pass')

def isosceles(a,b,c):
    if a==b or b==c or a==c:
        print('pass')
        
def equilateral(a,b,c):
    if a==b and b==c:
        print('pass')
        
def error(a,b,c):
    if a + b < c or a + b == c:
        print('pass')
        
def angle_total(a,b,c):
    if a+b+c==180:
        print('pass')
        
def area(base,height):
    if base*height/2:
        print('pass')
